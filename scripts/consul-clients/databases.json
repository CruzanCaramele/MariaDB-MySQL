{
	
	"ui"                     : true,
	"bind_addr"              : "0.0.0.0",
	"datacenter"             : "dc1",
	"data_dir"               : "/var/consul",
	"log_level"              : "INFO",
	"enable_syslog"          : true,
	"enable_debug"           : true,
	"server"                 : false,
	"leave_on_terminate"     : true,
	"skip_leave_on_interrupt": true,
	"rejoin_after_leave"     : true,
	"retry_join"             : ["10.0.0.95"],

	"service": {

		"name"   : "MariaDB",
		"tags"   : ["MariaDB"],
		"checks" : [
			{
				"script"   : "systemctl status mariadb",
				"interval" : "10s"
			}
		]
	}
}